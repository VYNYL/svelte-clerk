{
	"name": "svelte-clerk",
	"version": "0.10.0",
	"license": "MIT",
	"packageManager": "pnpm@9.13.2",
	"homepage": "https://github.com/wobsoriano/svelte-clerk#readme",
	"keywords": [
		"clerk",
		"typescript",
		"svelte",
		"auth",
		"authentication",
		"passwordless",
		"session",
		"jwt"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wobsoriano/svelte-clerk.git"
	},
	"bugs": "https://github.com/wobsoriano/svelte-clerk/issues",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"lint": "prettier --check . && eslint .",
		"release": "pnpm build && changeset publish",
		"format": "prettier --write .",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./client": {
			"types": "./dist/client/index.d.ts",
			"svelte": "./dist/client/index.js"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"svelte": "./dist/server/index.js"
		},
		"./env": "./dist/env.d.ts"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"typesVersions": {
		">4.0": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"components": [
				"./dist/components/index.d.ts"
			],
			"server": [
				"./dist/server/index.d.ts"
			],
			"env.d.ts": [
				"./dist/env.d.ts"
			]
		}
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.3.6",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/set-cookie-parser": "^2.4.10",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.47",
		"publint": "^0.2.11",
		"svelte": "^5.20.0",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^3.4.14",
		"tslib": "^2.8.0",
		"vite": "^6.1.0",
		"@changesets/cli": "^2.27.9",
		"@types/eslint": "^9.6.1",
		"eslint": "^9.13.0",
		"globals": "^15.11.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.11.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@clerk/backend": "^1.25.1",
		"@clerk/shared": "^3.0.0",
		"@clerk/types": "^4.47.0",
		"set-cookie-parser": "^2.7.1"
	}
}
